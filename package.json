  },
  "scripts": {
    "start": "concurrently --raw 'npm:react' 'npm:deploy'",
    "dev": "concurrently --raw 'npm:react' 'npm:server'",
    "react": "react-scripts start",
    "server": "cd server/ && npm run server",
    "start:sudo": "concurrently --raw 'npm:react:sudo' 'npm:deploy:sudo'",
    "dev:sudo": "concurrently --raw 'npm:react:sudo' 'npm:server:sudo'",
    "react:sudo": "sudo node ./node_modules/.bin/react-scripts start",
    "server:sudo": "cd server/ && npm run server:sudo"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
