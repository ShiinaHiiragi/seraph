    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.8",
    "@mui/icons-material": "^5.14.8",
    "@mui/joy": "^5.0.0-beta.5",
    "@mui/material": "^5.14.9",
    "@testing-library/jest-dom": "^5.17.0",
